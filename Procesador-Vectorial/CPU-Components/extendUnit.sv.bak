module extendUnit #(parameter N=18)(
	input logic [9:0] immediate,
	output logic [7:0][N-1:0] extended 
	);
	
	assign extended[0] = immediate[0:7];
	assign extended[1] = 8'b0;
	assign extended[2] = 8'b0;
	assign extended[3] = 8'b0;
	assign extended[4] = 8'b0;
	assign extended[5] = 8'b0;
	assign extended[6] = 8'b0;
	assign extended[7] = 8'b0;
	
endmodule 